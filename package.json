    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "mongoose": "^7.0.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "undefined": "^0.1.0",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
